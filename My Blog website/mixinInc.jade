mixin side(image, tit)
    .panel-body#firstPanel
       .media
         .media-left.media-middle
            img.media-object(src="#{image}", alt="ws", width=50)
         .media-body
           h4.media-heading #{tit}
           p Sed mantic punc
           
mixin minWell(text)
   .mw #{text}
      
